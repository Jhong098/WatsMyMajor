{
  "name": "WatsMyMajor",
  "version": "1.0.0",
  "description": "A web app that helps Waterloo students pick their courses",
  "engines": {
    "node": "6.11.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "build": "cd react-ui/ && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "firebase-admin": "^5.12.1",
    "moment": "^2.22.1",
    "request": "^2.86.0",
    "uwaterloo-api": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react",
    "waterloo"
  ],
  "devDependencies": {}
}
